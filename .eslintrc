{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mongo": true,
    "serviceworker": true
  },
  "rules": {
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": [
      2,
      {
        "exceptions": ["Route"]
      }
    ]
  }
}
